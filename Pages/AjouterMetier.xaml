<UserControl x:Class="AirAtlantique.Pages.AjouterMetier"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AirAtlantique.Pages"
             mc:Ignorable="d" 
             d:DesignHeight="345" d:DesignWidth="396">
    <Grid>
        <Label x:Name="label" Content="Métiers" HorizontalAlignment="Left" Height="23" Margin="21,11,0,0" VerticalAlignment="Top" Width="70"/>
        <TextBox x:Name="addMetier" HorizontalAlignment="Left" Height="23" Margin="159,56,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="185"/>
        <Button x:Name="button" Content="Ajouter un métier" HorizontalAlignment="Left" Margin="211,90,0,0" VerticalAlignment="Top" Width="86" FontSize="6.667" Height="21" Click="button_Click"/>
        <ListBox x:Name="listeMetiers" HorizontalAlignment="Left" Height="203" Margin="21,39,0,0" VerticalAlignment="Top" Width="115" />
        <TextBlock x:Name="alertEmpty" HorizontalAlignment="Left" Margin="159,30,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="185"/>

    </Grid>
</UserControl>
